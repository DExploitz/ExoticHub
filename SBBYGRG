function tp()
      local workspace = game:GetService("Workspace")
      if _G.tpname == "Brazil" then
            game.Players.LocalPlayer.Character:MoveTo(workspace.BrazilPosition.Position)
      end
      if _G.tpname == "Dimension" then
            game.Players.LocalPlayer.Character:MoveTo(workspace.DimensionPosition.Position)
      end
      if _G.tpname == "Backrooms" then
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-1465, 5177, -76))
      end
      if _G.tpname == "School" then
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(98, 295, 132))
      end
      if _G.tpname == "Castle" then
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(104, 294, -441))
      end
      if _G.tpname == "Volcano" then
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(323, 319, -20))
      end
      if _G.tpname == "Church" then
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(-169, 327, 19))
      end
      if _G.tpname == "Secret Obby" then
            game.Players.LocalPlayer.Character:MoveTo(Vector3.new(96, 316, 44293))
      end
end
      
