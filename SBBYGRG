function badge()
      if _G.badgename == "Asylum" then
            local teleUI = game:GetService("Workspace").Extra.Stuff.teleportPart.UI:Clone()
            local plr = game.Players.LocalPlayer
            teleUI.Parent = plr.PlayerGui
      end
      if _G.badgename == "Gunslinger" then
            local hat = game:GetService("Workspace").GunslingerBadge.TouchInterest
            local plr = game.Players.LocalPlayer
            firetouchinterest(plr, hat.Parent, 0)
            task.wait(0.1)
            firetouchinterest(plr, hat.Parent, 1)
      end
end

function kill()
      if _G.killmode == "all" then
            for i, v in pairs(game.Players:GetPlayers()) do
                  if v.Name ~= game.Players.LocalPlayer.Name then
                        local kargs = {
                              [1] = "slash",
                              [2] = v.Character,
                              [3] = Vector3.new(-9999999999999999999999999, -99999999999999999999999999, -99999999999999999999999999)
                            }
                        for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetDescendants()) do
                              if v:IsA("Tool") and v:FindFirstChild("Event") then
                                    v.Event:FireServer(unpack(kargs))
                              end
                        end
                        for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
                              if v:IsA("Tool") and v:FindFirstChild("Event") then
                                    v.Event:FireServer(unpack(kargs))
                              end
                        end
                  end
            end
      end
      if _G.killmode == "single" then
            if game:GetService("Players"):FindFirstChild(_G.plrname) then
                  local kargs2 = {
                        [1] = "slash",
                        [2] = game:GetService("Players"):FindFirstChild(_G.plrname).Character,
                        [3] = Vector3.new(-9999999999999999999999999, -99999999999999999999999999, -99999999999999999999999999)
                      }
                  for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetDescendants()) do
                        if v:IsA("Tool") and v:FindFirstChild("Event") then
                             v.Event:FireServer(unpack(kargs2))
                        end
                  end
                  for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
                       if v:IsA("Tool") and v:FindFirstChild("Event") then
                              v.Event:FireServer(unpack(kargs2))
                       end
                  end
            else
                  OrionLib:MakeNotification({
	                   Name = "Error",
                   	Content = "Player Not Found!",
	                   Image = "rbxassetid://4483345998",
                   	Time = 2
                  })
            end
      end
end

function slap()
      while _G.slap == true do
            for i, v in pairs(game.Players:GetPlayers()) do
                  if v.Name ~= game.Players.LocalPlayer.Name then
                        local sargs = {
                              [1] = "slash",
                              [2] = v.Character,
                              [3] = Vector3.new(2.305302858352661, 4.531458941414712e-08, -3.2688803672790527)
                            }
                        for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetDescendants()) do
                              if v:IsA("Tool") and v:FindFirstChild("Event") then
                                    v.Event:FireServer(unpack(sargs))
                              end
                        end
                        for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
                              if v:IsA("Tool") and v:FindFirstChild("Event") then
                                    v.Event:FireServer(unpack(sargs))
                              end
                        end
                  end
            end
            task.wait(0.0000000000000000000000001)
      end
end

function admindetect()
      while _G.antikick == true do
            if game.Players:FindFirstChild("DevOfficiallyReality") or game.Players:FindFirstChild("itz_galaxymidnight10") or game.Players:FindFirstChild("MONsocool666") or game.Players:FindFirstChild("almondandraisinaaaaa") or game.Players:FindFirstChild("sapotals2") or game.Players:FindFirstChild("25boi1") or game.Players:FindFirstChild("Emanman2014")  then
                  OrionLib:MakeNotification({
	                          Name = "Admin Detection",
	                          Content = "Admin Joined! Kicking player...",
	                          Image = "rbxassetid://4483345998",
                          	Time = 2
                       })
                   task.wait(0.5)
                   game.Players.LocalPlayer:Kick("Admin Joined. Kicked to avoid admin")
            end
            task.wait(0.1)
       end
end

function nolimit()
      while _G.nolimit == true do
            task.wait(0.000001)
            if game:GetService("StarterPlayer").StarterCharacterScripts:FindFirstChild("InventoryLimiter") then
                  game:GetService("StarterPlayer").StarterCharacterScripts:FindFirstChild("InventoryLimiter"):Destroy()
            end
            if game.Players.LocalPlayer.Character:FindFirstChild("InventoryLimiter") then
                  game.Players.LocalPlayer.Character:FindFirstChild("InventoryLimiter"):Destroy()
            end
      end
end

function nospeed()
      while _G.nospeed == true do
            task.wait(0.000001)
            if game:GetService("StarterPlayer").StarterPlayerScripts:FindFirstChild("SpeedCamera") then
                  game:GetService("StarterPlayer").StarterPlayerScripts:FindFirstChild("SpeedCamera"):Destroy()
            end
            if game.Players.LocalPlayer.PlayerScripts:FindFirstChild("SpeedCamera") then
                  game.Players.LocalPlayer.PlayerScripts:FindFirstChild("SpeedCamera"):Destroy()
            end
      end
end

function novoid()
    if _G.antivoid == true then
        local part = Instance.new("Part", game.Workspace)
        part.Name = "AntiVoid"
        part.Anchored = true
        part.Position = Vector3.new(103, 270, -147)
        part.Size = Vector3.new(2048, 10, 2048)
        part.Color = Color3.fromRGB(0, 0, 0)
        part.Material = "Glass"
        part.Transparency = 0.7
        part.Touched:Connect(function(hit)
            local plr = game.Players.LocalPlayer
            plr.Character:MoveTo(Vector3.new(103, 294, -147))
        end)
        OrionLib:MakeNotification({
	        Name = "Added Anti Void",
        	Content = "Successfully added anti void!",
        	Image = "rbxassetid://4483345998",
        	Time = 5
        })
    elseif _G.antivoid == false then
        if game:GetService("Workspace"):FindFirstChild("AntiVoid") then
            game:GetService("Workspace"):FindFirstChild("AntiVoid"):Destroy()
            OrionLib:MakeNotification({
	            Name = "Deleted Anti Void",
            	Content = "Successfully deleted anti void due to toggle being false!",
            	Image = "rbxassetid://4483345998",
            	Time = 5
            })
        end
    end
end

function setpower()
    while _G.setpower == true and task.wait(0.000001) do
        for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetDescendants()) do
            if v:IsA("Tool") and v:FindFirstChild("Event") then
                v.Power.Value = Value
            end
        end
        for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
            if v:IsA("Tool") and v:FindFirstChild("Event") then
                v.Power.Value = Value
            end
        end
    end
end

function setspeed()
    while _G.setspeed == true and task.wait(0.000001) do
        for i, v in pairs(game:GetService("Players").LocalPlayer.Backpack:GetDescendants()) do
            if v:IsA("Tool") and v:FindFirstChild("Event") then
                v.Speed.Value = Value
            end
        end
        for i, v in pairs(game:GetService("Players").LocalPlayer.Character:GetDescendants()) do
            if v:IsA("Tool") and v:FindFirstChild("Event") then
                v.Speed.Value = Value
            end
        end
    end
end

